{
  "version": 2,
  "builds": [
    { "src": "mysite/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "mysite/wsgi.py" }
  ],
  "scripts": {
    "build": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
  }
}
